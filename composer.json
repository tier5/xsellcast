{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "mpociot/laravel-apidoc-generator": "1.*",
        "prettus/l5-repository": "^2.6",
        "zizaco/entrust": "5.2.x-dev",
        "doctrine/dbal": "~2.3",
        "league/fractal": "0.15.*",
        "lavary/laravel-menu": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "snowfire/beautymail": "dev-master",
        "laravel/socialite": "~2.0",
        "kodeine/laravel-meta": "dev-master",
        "lucadegasperi/oauth2-server-laravel": "5.1.*",
        "mandrill/mandrill": "^1.0",
        "stevebauman/eloquenttable": "1.1.*",
        "laravelcollective/html": "^5.2.0",
        "intervention/image": "^2.3",
        "lord/laroute" : "2.*",
        "fullcontact/sdk" : "dev-master",
        "vnn/wordpress-rest-api-client": "^1.1",
        "cmgmyr/messenger": "~2.0",
        "stevebauman/location": "2.0.*",
        "propaganistas/laravel-phone": "^2.8",
        "cviebrock/eloquent-sluggable": "^4.0",
        "barryvdh/laravel-debugbar": "^2.3",
        "behat/behat": "3.*@stable",
        "emuse/behat-html-formatter": "0.1.*",
        "maatwebsite/excel": "~2.1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "http://github.com/fullcontact/fullcontact-api-php"
        },
        {
            "type": "vcs",
            "url": "http://github.com/WP-API/WP-API"
        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "bin-dir": "bin/"
    }
}
